{
  "name": "realblocks-test-task",
  "version": "1.0.0",
  "description": "RealBlocks test task",
  "repository": {
    "type": "git",
    "url": "https://github.com/puku/RealBlocks"
  },
  "main": "dist/index.js",
  "author": "Ivan Liauchuk",
  "license": "MIT",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose --detectOpenHandles",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "eslint": "eslint --fix \"**/*.js\" \"**/*.ts\"",
    "eslint-ci": "eslint --fix-dry-run \"**/*.js\" \"**/*.ts\"",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "migrate-up": "migrate up --compiler=\"ts:./typescript-compiler.js\"",
    "migrate-list": "migrate list --compiler=\"ts:./typescript-compiler.js\"",
    "migrate-down": "migrate down --compiler=\"ts:./typescript-compiler.js\""
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "dotenv": "^7.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "migrate": "^1.6.2",
    "mongodb": "^3.2.2",
    "ts-node": "^8.0.3"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.11",
    "@types/mongodb": "^3.1.22",
    "@types/supertest": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "concurrently": "^4.1.0",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-eslint-plugin": "^2.0.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "jest": "^24.5.0",
    "nodemon": "^1.18.10",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.4000"
  }
}
